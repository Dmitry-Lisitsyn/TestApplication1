@model IEnumerable<TestApplication1.Models.DataModel>
@{
    ViewData["Title"] = "Home Page";
    string[] TableHeaders = new string[] { "Id", "Name", "Code" };
}

    <table id="myTableData" class="table table-bordered table-hover table-sm">
        <thead>
            <tr>
                @{
                    foreach (var head in TableHeaders)
                    {
                        <th class="text-center">
                            @head
                        </th>
                    }

                 }
            </tr>
        </thead>
        @{
            if (Model != null)
            {
                foreach (var Data in Model)
                {
                    <tr>
                        <th> @Data.Id</th>
                        <th> @Data.Name</th>
                        <th> @Data.Code</th>
                    </tr>
                }
            }

         }


    </table>
